query/search/result

class Main

    class query
        def edit
            --field -f 
                all - default
                [field]

        def clear


    class search 
        def do
            --save -s
                [resultName]

    class result
        class format
            def sort
                --by -b
                    [option]
            def slice
                --from -f
                    firsts - default
                    [option]
                --number -n
        class save
            def json

            def excel
        
        def see
            --detail-level -d

    def quit

    def restart